{
  "name": "lsos",
  "version": "0.1.0",
  "dependencies": {
    "find-up": "^5.0.0"
  },
  "main": "dist/verify.js",
  "bin": {
    "lsos": "dist/cli/index.js"
  },
  "types": "dist/verify.d.ts",
  "scripts": {
    "// Post-install": "",
    "postinstall": "node -e \"try{require('./dist/postinstall/index.js')}catch(err){console.log('====== Warning ======');console.log(err);}\"",
    "// Build": "",
    "build": "yarn build:clean && yarn build:transpile && yarn build:binaries",
    "build:clean": "rm -rf dist/",
    "build:transpile": "tsc",
    "build:binaries": "(mkdir -p dist/cli/ && mkdir -p dist/postinstall/ && touch dist/cli/index.js dist/postinstall/index.js && chmod 775 dist/cli/index.js dist/postinstall/index.js)",
    "// Release": "",
    "release": "yarn build && npm run publish"
  },
  "devDependencies": {
    "@types/node": "^14.14.6",
    "typescript": "^4.0.2"
  },
  "files": [
    "/dist/",
    "/src/"
  ],
  "repository": "https://github.com/Lsos/lsos-js",
  "publishConfig": {
    "access": "public"
  }
}
